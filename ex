#include <iostream>
using namespace std;
int main()
{
int a = 5 ;
if ( 3 =< a < 5) {
cout << " YẾU " << endl;
}
else if( a < 3 ) {
cout << " KÉM " << endl;
}
else if( 5 < a < 7 ) {
cout << " TRUNG BÌNH " << endl;
}
else if ( 7 < a < 8) {
cout << " KHÁ " << endl;
}
else if ( 8 < a < 10) {
cout << " GIỎI " << endl;
}
else ( a = 10 ) {
cout << " GIỎI " << endl;
}
}
